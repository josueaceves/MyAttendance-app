<p id="notice"><%= notice %></p>

<h1>Listing Choir Members</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Voice</th>
      <th>Side</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @choir_members.each do |choir_member| %>
      <tr>
        <td><%= choir_member.first_name %></td>
        <td><%= choir_member.last_name %></td>
        <td><%= choir_member.voice %></td>
        <td><%= choir_member.side %></td>


      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Choir member', new_user_choir_choir_member_path(1,1) %>
